{
  "manifest_version": 2,
  "name": "Photo Depot",
  "version": "1.0",
  "description": "Click and save your image urls and view your collection at Photo Depot.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./app.js"],
      "css": ["./styles.scss"]
    }
  ],
  "key": "",
  "oauth2": {
    "client_id": "265226410890-17pudtqfsqae1nbii53u8nbrlvb5ej18.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "icons": {
    "128": "./images/photo-depot-128px.png"
  },
  "browser_action": {
    "default_title": "Tool Tip: TEST default_title",      
    "default_popup": "./popup.html",
    "default_icon": {                   
      "16": "./images/photo-depot-16px.png",
      "48": "./images/photo-depot-48px.png",
      "128": "./images/photo-depot-128px.png"
    }
  },
  "background": {
    "scripts": [
      "eventPage.js",
      "jwt-decode.js"
    ],
    "persistent": false
  },
  "permissions": [
    "identity",
    "contextMenus"
  ]
}